<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_graph"
    app:startDestination="@id/mainFragment">

    <fragment
        android:id="@+id/mainFragment"
        android:name="com.example.hw.MainFragment"
        android:label="fragment_main"
        tools:layout="@layout/fragment_main" >
        <action
            android:id="@+id/action_mainFragment_to_asanFragment"
            app:destination="@id/asanFragment" />
    </fragment>
    <fragment
        android:id="@+id/exersiseFragment"
        android:name="com.example.hw.ExerciseFragment"
        android:label="fragment_exersise"
        tools:layout="@layout/fragment_exercise" />
    <fragment
        android:id="@+id/profileFragment"
        android:name="com.example.hw.ProfileFragment"
        android:label="fragment_profile"
        tools:layout="@layout/fragment_profile" />
    <fragment
        android:id="@+id/progressFragment"
        android:name="com.example.hw.ProgressFragment"
        android:label="fragment_progress"
        tools:layout="@layout/fragment_progress" />
    <fragment
        android:id="@+id/yogovsryChatFragment"
        android:name="com.example.hw.YogovsryChatFragment"
        android:label="fragment_yogovsry_chat"
        tools:layout="@layout/fragment_yogovsry_chat" />
    <fragment
        android:id="@+id/asanFragment"
        android:name="com.example.hw.AsanasFragment"
        android:label="fragment_asan"
        tools:layout="@layout/fragment_asanas" />
</navigation>